   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "m = 0\n",
    "n = 100000\n",
    "for i in range(n):\n",
    "    phi = np.random.random((3,1)) * 2 * np.pi\n",
    "    k = 2 * np.pi - (max(phi) - min(phi))\n",
    "    m += np.linalg.norm(k)\n",
    "m/n"
   ]
  }
